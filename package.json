{
  "name": "taverasmisael-website",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "packageManager": "pnpm@8.13.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm astro:check && astro build",
    "build:local": "pnpm astro:check && rm -rf ./dist && LOCAL_BUILD=true astro build",
    "build:preview": "pnpm build:local && pnpm preview",
    "preview": "LOCAL_BUILD=true astro preview",
    "astro": "astro",
    "astro:check": "astro check && tsc --noEmit",
    "format": "prettier -c .",
    "format:save": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "pnpm format:save && npm run lint:fix"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.0.2",
    "@astrojs/node": "^7.0.2",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "^4.0.1",
    "@astrojs/solid-js": "^3.0.2",
    "@astrojs/tailwind": "^5.0.4",
    "@astrojs/ts-plugin": "^1.3.1",
    "@astrojs/vercel": "^6.1.0",
    "@fontsource-variable/fira-code": "^5.0.16",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource-variable/red-hat-display": "^5.0.19",
    "@kobalte/core": "^0.11.2",
    "@solid-primitives/fetch": "^2.4.7",
    "@solid-primitives/scheduled": "^1.4.1",
    "algoliasearch": "^4.22.0",
    "astro": "^4.0.7",
    "canvas-confetti": "^1.9.2",
    "rambda": "^8.6.0",
    "satori-html": "^0.3.2",
    "solid-js": "^1.8.7",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
