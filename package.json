{
  "name": "taverasmisael-website",
  "type": "module",
  "version": "0.5.1",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=21.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm astro:check && astro build",
    "build:local": "pnpm astro:check && rm -rf ./dist && LOCAL_BUILD=true astro build",
    "build:preview": "pnpm build:local && pnpm preview",
    "preview": "LOCAL_BUILD=true astro preview",
    "astro": "astro",
    "astro:check": "astro check && tsc --noEmit",
    "format": "prettier -c .",
    "format:save": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "pnpm format:save && pnpm run lint:fix"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/node": "^8.2.3",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/solid-js": "^4.0.1",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.5.3",
    "@astrojs/vercel": "^7.3.4",
    "@fontsource-variable/fira-code": "^5.0.16",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource-variable/red-hat-display": "^5.0.19",
    "@kobalte/core": "^0.13.7",
    "@solid-primitives/fetch": "^2.4.8",
    "@solid-primitives/scheduled": "^1.4.2",
    "algoliasearch": "^5.8.1",
    "astro": "^4.4.9",
    "canvas-confetti": "^1.9.2",
    "rambda": "^9.1.0",
    "satori-html": "^0.3.2",
    "solid-js": "^1.8.15",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.0",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^22.7.5",
    "@types/remove-markdown": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "remove-markdown": "^0.5.0",
    "satori": "^0.11.2",
    "sharp": "^0.33.2",
    "sitemap": "^8.0.0",
    "string-byte-slice": "^3.0.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.1.4"
  }
