{
  "name": "taverasmisael-website",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm astro:check && astro build",
    "build:local": "pnpm astro:check && rm -rf ./dist && LOCAL_BUILD=true astro build",
    "build:preview": "pnpm build:local && pnpm preview",
    "preview": "LOCAL_BUILD=true astro preview",
    "astro": "astro",
    "astro:check": "astro check && tsc --noEmit",
    "format": "prettier -c .",
    "format:save": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "pnpm format:save && npm run lint:fix"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/node": "^5.3.0",
    "@astrojs/prefetch": "^0.2.3",
    "@astrojs/rss": "^2.4.3",
    "@astrojs/solid-js": "^2.2.0",
    "@astrojs/tailwind": "^4.0.0",
    "@astrojs/ts-plugin": "^1.0.10",
    "@astrojs/vercel": "^3.5.1",
    "@fontsource-variable/fira-code": "^5.0.3",
    "@fontsource-variable/inter": "^5.0.3",
    "@fontsource-variable/red-hat-display": "^5.0.4",
    "@kobalte/core": "^0.9.7",
    "@solid-primitives/fetch": "^2.4.6",
    "@solid-primitives/scheduled": "^1.3.2",
    "algoliasearch": "^4.18.0",
    "astro": "^2.7.1",
    "canvas-confetti": "^1.6.0",
    "rambda": "^7.5.0",
    "satori-html": "^0.3.2",
    "solid-js": "^1.7.7",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.4.1",
    "@tailwindcss/typography": "^0.5.9",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^20.3.2",
    "@types/remove-markdown": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "remove-markdown": "^0.5.0",
    "satori": "^0.10.1",
    "sitemap": "^7.1.1",
    "string-byte-slice": "^2.0.0",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.3.9"
  }
